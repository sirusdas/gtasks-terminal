    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^6.0.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "sqlite3": "^5.1.6",
    "socket.io": "^4.6.1",
    "@types/ws": "^8.5.4",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/compression": "^1.7.2",
    "@types/sqlite3": "^3.1.8"
  },
  "devDependencies": {
    "@types/react-beautiful-dnd": "^13.1.4",
