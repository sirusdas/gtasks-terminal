{
  "name": "gtasks-dashboard",
  "version": "2.0.0",
  "description": "Advanced Google Tasks Dashboard with Multi-Account Support and Real-time Sync",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "node server/index.js",
    "build": "tsc && vite build",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "install:all": "npm install && cd server && npm install",
    "setup": "npm run install:all && npm run build",
    "docker:build": "docker build -t gtasks-dashboard .",
    "docker:run": "docker run -p 3000:3000 -p 8080:8080 gtasks-dashboard"
  },
  "keywords": [
    "google-tasks",
    "dashboard",
    "react",
    "typescript",
    "websocket",
    "mcp",
    "multi-account"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/node": "^18.14.0",
    "typescript": "^4.9.4",
    "vite": "^4.1.0",
    "@vitejs/plugin-react": "^3.1.0",
    "tailwindcss": "^3.2.6",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "lucide-react": "^0.263.1",
    "recharts": "^2.5.0",
    "react-force-graph-2d": "^1.23.0",
    "react-data-table-component": "^7.5.3",
    "styled-components": "^5.3.6",
    "@headlessui/react": "^1.7.13",
    "@heroicons/react": "^2.0.16",
    "socket.io-client": "^4.6.1",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.39.3",
    "zustand": "^4.3.2",
    "framer-motion": "^10.0.1",
    "react-hook-form": "^7.43.5",
    "zod": "^3.20.6",
